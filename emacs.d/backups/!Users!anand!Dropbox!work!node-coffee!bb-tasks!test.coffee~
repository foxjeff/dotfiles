Backbone = require("backbone")

#module.exports =
Person =
  Backbone.Model.extend
    initialize: ->
      console.log "Welcome to this world"
    defaults:
      name: "Blerg2"
      age: 23


# or we can set afterwards, these operations are equivelent
personA = new Person()
personA.set
  name: "Thomas"
  age: 67

module.exports.Person=Person
module.exports.personA=personA