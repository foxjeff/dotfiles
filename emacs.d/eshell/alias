alias vo view-file-other-window $1
alias v view-file $1
alias do dired-other-window $1
alias d dired $1
alias ffo find-file-other-window $1
alias ff find-file $1
alias ls /usr/local/opt/coreutils/libexec/gnubin/ls -p --group-directories-first --color=auto $*
alias node NODE_NO_READLINE=1 node
alias catfoo cat $1 > #<buffer foo>
